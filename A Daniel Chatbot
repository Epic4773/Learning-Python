from colorama import init, Fore, Style
import random
import time
import sys

def chatbot():
  init()
  colors = {
    'red': Fore.RED,
    'green': Fore.GREEN,
    'yellow': Fore.YELLOW,
    'blue': Fore.BLUE,
    'magenta': Fore.MAGENTA,
    'cyan': Fore.CYAN,
    'white': Fore.WHITE,
  }
  our_color = input('DanielGPT: Before be begin, choose a color for the text to be (red, green, yellow, blue, magenta, cyan, (default) white): \n\nYou: ')
  color_code = colors.get(our_color.lower(), Fore.RESET)
  if our_color not in colors:
    input('\nDanielGPT: Sorry, that\'s not an option, I\'ll default to white.\n\nYou: Okay, you\'re awesome.\n\nDanielGPT: I know, right?\n\nYou: ')
