      print(Fore.GREEN + '\nDanielGPT: Okay.\n' + Fore.RESET + color_code)
    elif country.lower() in ['roll', 'dice', 'die']:
      sides = int(input(f"\nDanielGPT: How many sides do you want the die to have?\n\n{name}: "))
      if type(sides) != int:
        print('DanielGPT: That\'s not a number so I\'ll default to 6. The die is rolling...')
        sides = 6
      else:
        number = random.randint(1, sides)
      print(f"\nDanielGPT: {number}\n")
    elif country.lower() in ['time', 'print', 'letter', 'letters', 'easter egg']:
      print('\nDanielGPT: Okay, we will be printing letters with a time feature. Ready?\n')
      input(f"{name}: ")
      while True:
        duration = float(input(f"\nDanielGPT: Enter the number of seconds to print each letter: \n{name}: "))
        case = input(f"\nDanielGPT: Now enter 'u' for uppercase or 'l' for lowercase letters: \n\n{name}: ")
        print()
        if case == 'u':
          start = ord('A')
          end = ord('Z')
        elif case == 'l':
          start = ord('a')
          end = ord('z')
        else:
          print(Fore.RED + "\nDanielGPT: Invalid input." + Fore.RESET + color_code)
          continue
        for i in range(start, end+1):
          letter = chr(i)
          sys.stdout.write(letter)
          sys.stdout.flush()
          time.sleep(duration)
        choice = input(f"\n\nDanielGPT: Enter 'y' to try again, or any other key to quit:\n\n{name}: ")
        if choice != 'y':
          print('\nDanielGPT: Okay, thanks for trying out the printing letters feature! Back to the countries.\n')
          break
    elif not country:
      print(Fore.RED + '\nDanielGPT: You did not enter anything. Please try again.\n' + Fore.RESET + color_code)
    else:
      print(Fore.RED + "\nDanielGPT: I'm sorry, either that isn\'t a country or you misspelled.\n" + Fore.RESET + color_code)
